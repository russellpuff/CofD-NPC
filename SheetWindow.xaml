          Height="770"
          Width="1060">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="250*" />
            <ColumnDefinition Width="4" />
            <ColumnDefinition Width="806*" />
        </Grid.ColumnDefinitions>
        <Rectangle x:Name="swVertLine3"
                   Grid.Column="1"
                   Height="760"
                   Stroke="#FF7F7F7F"
                   VerticalAlignment="Center"
                   Width="1" />
