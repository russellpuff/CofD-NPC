                               FontSize="14"
                               Grid.Column="2"
                               Grid.ColumnSpan="2"
                               Grid.Row="5"
                               UpDownButtonsWidth="20"
                               ParsingNumberStyle="Integer"
                               Minimum="0"
                               Maximum="100"
                               TrackMouseWheelWhenMouseOver="True"
                               Style="{StaticResource MaterialDesignNumericUpDown}"
                               RenderTransformOrigin="0.5,0.5"
                               Padding="0,0,0,0"
                               Foreground="White"
                               Value="0"
                               TextAlignment="Right"
                               HorizontalAlignment="Right"
                               Width="85"
                               MouseEnter="RollRectangle_MouseEnter"
                               MouseLeave="RollRectangle_MouseLeave">
            </mah:NumericUpDown>
