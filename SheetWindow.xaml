                    Grid.Column="1"
                    Grid.Row="2"
                    Grid.ColumnSpan="4"
                    Style="{StaticResource MaterialDesignIconButton}"
                    VerticalAlignment="Bottom"
                    Width="40"
                    Height="40"
                    Click="AddNPCButton_Click">
                <md:PackIcon Kind="Plus"
                             Width="32"
                             Height="32" />
            </Button>
            <TextBlock x:Name="swQuickRollLabel"
                       Text="Quick Roll"
                       HorizontalAlignment="Center"
                       VerticalAlignment="Center"
                       FontFamily="Elephant"
                       FontSize="20"
                       Foreground="White"
                       Grid.Row="4"
                       Grid.Column="1"
                       Grid.ColumnSpan="4"
                       MouseEnter="RollRectangle_MouseEnter"
                       MouseLeave="RollRectangle_MouseLeave" />
