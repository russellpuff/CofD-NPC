              Width="250"
              Height="770"
              Background="#FF303030">
            <Grid.RowDefinitions>
                <RowDefinition Height="10" />
                <RowDefinition Height="32" />
                <RowDefinition Height="588" />
                <RowDefinition Height="10" />
                <RowDefinition Height="30" />
                <RowDefinition Height="30" />
                <RowDefinition Height="30" />
                <RowDefinition Height="30" />
                <RowDefinition Height="10" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="10" />
                <ColumnDefinition Width="24" />
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition Width="115" />
                <ColumnDefinition Width="10" />
            </Grid.ColumnDefinitions>
            <TextBox x:Name="swNPCSearchTextBox"
                     md:HintAssist.Hint="Search NPC"
                     FontSize="18"
                     Foreground="White"
                     Grid.Column="1"
                     Grid.Row="1"
                     Grid.ColumnSpan="4">
            </TextBox>
            <DataGrid x:Name="swDataGrid"
                      Grid.Column="1"
                      Grid.Row="2"
                      Grid.ColumnSpan="4"
                      Background="#FF424242"
                      Foreground="White"
                      AutoGenerateColumns="False"
                      CanUserAddRows="false"
                      IsReadOnly="True"
                      CanUserDeleteRows="False"
                      CanUserResizeRows="False">
                <DataGrid.ContextMenu>
                    <ContextMenu Name="swDataGridContextMenu"
                                 Opened="DataGridContextMenu_Opened"
                                 StaysOpen="true">
                        <MenuItem Header="Open"
                                  Click="DataGridMenuItem_Click" />
                        <MenuItem Header="Delete"
                                  Click="DataGridMenuItem_Click" />
                        <MenuItem Header="Rename"
                                  Click="DataGridMenuItem_Click" />
                    </ContextMenu>
                </DataGrid.ContextMenu>
                <DataGrid.Columns>
                    <DataGridTextColumn CanUserResize="False"
                                        CanUserReorder="False"
                                        Header="Name"
                                        IsReadOnly="True"
                                        SortDirection="Ascending"
                                        Width="130"
                                        Binding="{Binding Name}" />
                    <DataGridTextColumn CanUserResize="False"
                                        CanUserReorder="False"
                                        Header="Description"
                                        IsReadOnly="True"
                                        Width="100"
                                        Binding="{Binding Description}" />
                    <DataGridTextColumn CanUserResize="False"
                                        CanUserReorder="False"
                                        Header="ID"
                                        Visibility="Hidden"
                                        Width="0"
                                        Binding="{Binding ID}" />
                </DataGrid.Columns>
            </DataGrid>
