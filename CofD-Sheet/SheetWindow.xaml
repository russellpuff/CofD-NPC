<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        x:Name="Sheet_Window"
        x:Class="CofD_Sheet.SheetWindow"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="#FF303030"
        TextElement.FontWeight="Medium"
        mc:Ignorable="d"
        Title="CofD Character Sheet Manager"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        ResizeMode="CanResize"
        Closing="SheetWindow_Closing">
    <!--Doesn't really follow MVVM at all because I didn't know it when I started.-->
    <!--If this upsets you then respectfully, cry about it. Or fix it yourself, either one.-->
    <ScrollViewer x:Name="swWindowScrollViewer"
                  HorizontalScrollBarVisibility="Auto"
                  VerticalScrollBarVisibility="Auto"
                  PreviewMouseDown="ScrollViewer_PreviewMouseDown"
                  PreviewMouseMove="ScrollViewer_PreviewMouseMove"
                  PreviewMouseUp="ScrollViewer_PreviewMouseUp">
        <Grid x:Name="swMajorGrid"
              Height="1016"
              Width="1920">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="400" />
                <ColumnDefinition Width="4" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Rectangle x:Name="swVertLine3"
                       Grid.Column="1"
                       Height="996"
                       Stroke="#FF7F7F7F"
                       VerticalAlignment="Center"
                       Margin="2,0,2,0" />
            <Grid x:Name="swLeftGrid"
                  Height="Auto"
                  Background="#FF303030">
                <Grid.RowDefinitions>
                    <RowDefinition Height="10" />
                    <RowDefinition Height="500" />
                    <RowDefinition Height="32" />
                    <RowDefinition Height="400*" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="100" />
                    <ColumnDefinition Width="10" />
                </Grid.ColumnDefinitions>
                <TextBox x:Name="swCharacterSearchBox"
                         md:HintAssist.Hint="Search Character"
                         FontSize="18"
                         Foreground="White"
                         Grid.Column="1"
                         Grid.Row="2"
                         TextChanged="SearchBar_TextChanged">
                </TextBox>
                <ComboBox x:Name="swCharacterFilterComboBox"
                          md:HintAssist.Hint="Filter Type"
                          Grid.Column="2"
                          Grid.Row="2"
                          FontSize="16"
                          Foreground="White"
                          VerticalAlignment="Bottom"
                          SelectionChanged="FilterComboBox_SelectionChanged">
                    <ComboBoxItem Content="&lt;No Filter&gt;" />
                    <ComboBoxItem Content="NPC" />
                    <ComboBoxItem Content="Mortal" />
                    <ComboBoxItem Content="Ephemeral" />
                    <ComboBoxItem Content="Vampire" />
                    <ComboBoxItem Content="Mage" />
                    <ComboBoxItem Content="Werewolf" />
                </ComboBox>
                <DataGrid x:Name="swCharactersDataGrid"
                          Grid.Column="1"
                          Grid.Row="3"
                          Grid.ColumnSpan="2"
                          Background="#FF424242"
                          Foreground="White"
                          AutoGenerateColumns="False"
                          CanUserAddRows="false"
                          IsReadOnly="True"
                          CanUserDeleteRows="False"
                          CanUserResizeRows="False"
                          RowStyle="{StaticResource DataGrdVisibilitySwitch}"
                          CellStyle="{StaticResource DataGridCellNoFocus}"
                          SelectionChanged="DataGrid_SelectionChanged"
                          PreviewMouseDown="DataGrid_PreviewMouseDown">
                    <DataGrid.Columns>
                        <DataGridTextColumn CanUserResize="False"
                                            CanUserReorder="False"
                                            Header="Name"
                                            IsReadOnly="True"
                                            SortDirection="Ascending"
                                            Width="*"
                                            Binding="{Binding Name}"
                                            FontSize="20" />
                        <DataGridTemplateColumn CanUserResize="False"
                                                CanUserReorder="False"
                                                IsReadOnly="True"
                                                Width="70"
                                                Header="  Type"
                                                SortMemberPath="TypeSortValue">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <Image Source="{Binding Image}"
                                           Stretch="Uniform">
                                        <Image.ToolTip>
                                            <ToolTip>
                                                <TextBlock Text="{Binding TypeToolTip}" />
                                            </ToolTip>
                                        </Image.ToolTip>
                                    </Image>
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTextColumn CanUserResize="False"
                                            CanUserReorder="False"
                                            Header="ID"
                                            Visibility="Hidden"
                                            Width="0"
                                            Binding="{Binding ID}" />
                    </DataGrid.Columns>
                    <DataGrid.ContextMenu>
                        <ContextMenu>
                            <MenuItem Header="Delete"
                                      Click="Delete_OnClick"
                                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                        </ContextMenu>
                    </DataGrid.ContextMenu>
                </DataGrid>
                <Ellipse x:Name="swAddNPCElipse"
                         Grid.Column="1"
                         Grid.Row="3"
                         Grid.ColumnSpan="2"
                         VerticalAlignment="Bottom"
                         Height="45"
                         Stroke="#FF303030"
                         Fill="#FF303030"
                         HorizontalAlignment="Stretch"
                         Width="50" />
                <Rectangle x:Name="swAddNPCRectangle"
                           Grid.Column="1"
                           Grid.Row="3"
                           Grid.ColumnSpan="2"
                           VerticalAlignment="Bottom"
                           Height="20"
                           Width="50"
                           Stroke="#FF303030"
                           Fill="#FF303030" />
                <Rectangle x:Name="swAddNPCRectangle2"
                           Grid.Column="1"
                           Grid.Row="3"
                           Grid.ColumnSpan="2"
                           VerticalAlignment="Bottom"
                           Height="10"
                           Stroke="#FF303030"
                           Fill="#FF303030" />
                <Button x:Name="swAddNPCButton"
                        Grid.Column="1"
                        Grid.Row="3"
                        Grid.ColumnSpan="2"
                        Style="{StaticResource MaterialDesignIconButton}"
                        VerticalAlignment="Bottom"
                        Width="40"
                        Height="40"
                        Click="AddCharacterButton_Click">
                    <md:PackIcon Kind="Plus"
                                 Width="32"
                                 Height="32" />
                </Button>
                <Image x:Name="swPortraitImage"
                       Grid.Column="1"
                       Grid.Row="1"
                       Grid.ColumnSpan="4"
                       MouseEnter="Rectangle_MouseEnter"
                       MouseLeave="Rectangle_MouseLeave"
                       MouseDown="Portrait_Click">
                    <Image.ContextMenu>
                        <ContextMenu>
                            <MenuItem Header="Choose Image"
                                      Click="ChooseImage_OnClick"
                                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            <MenuItem Header="Remove Image"
                                      Click="RemoveImage_OnClick"
                                      CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                        </ContextMenu>
                    </Image.ContextMenu>
                </Image>
                <Rectangle x:Name="swPortraitRectangle"
                           Grid.Column="1"
                           Grid.Row="1"
                           Grid.ColumnSpan="2"
                           Stroke="#FF707070"
                           RadiusX="5"
                           RadiusY="5"
                           MouseEnter="Rectangle_MouseEnter"
                           MouseLeave="Rectangle_MouseLeave" />
            </Grid>
            <Grid x:Name="swSheetGrid"
                  Grid.Column="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="300" />
                    <ColumnDefinition Width="4" />
                    <ColumnDefinition Width="280" />
                    <ColumnDefinition Width="4" />
                    <ColumnDefinition Width="260" />
                    <ColumnDefinition Width="4" />
                    <ColumnDefinition Width="300" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="770" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid x:Name="swInnerLeftGrid"
                      Margin="10"
                      Grid.Column="0"
                      ForceCursor="True">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="32" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="32" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="32" />
                        <RowDefinition Height="28" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="30" />
                        <RowDefinition Height="32" />
                        <RowDefinition Height="100*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="80*" />
                        <ColumnDefinition Width="200*" />
                    </Grid.ColumnDefinitions>
                    <Label x:Name="swNameLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Name:"
                           Grid.Column="0"
                           Grid.Row="0"
                           Padding="0,0,0,0" />
                    <Label x:Name="swTypeLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Type:"
                           Grid.Column="0"
                           Grid.Row="1"
                           Padding="0,0,0,0" />
                    <Label x:Name="swAgeLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Age:"
                           Grid.Column="0"
                           Grid.Row="2"
                           Padding="0,0,0,0" />
                    <Label x:Name="swVirtueLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Virtue:"
                           Grid.Column="0"
                           Grid.Row="3"
                           Padding="0,0,0,0" />
                    <Label x:Name="swViceLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Vice:"
                           Grid.Column="0"
                           Grid.Row="4"
                           Padding="0,0,0,0" />
                    <Label x:Name="swConceptLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Concept:"
                           Grid.Column="0"
                           Grid.Row="5"
                           Padding="0,0,0,0" />
                    <TextBox x:Name="swNameTextBox"
                             FontSize="16"
                             Grid.Column="1"
                             Grid.Row="0"
                             Padding="0,0,0,0"
                             TextChanged="TextBox_TextChanged">
                    </TextBox>
                    <ComboBox x:Name="swTypeComboBox"
                              FontSize="16"
                              Grid.Column="1"
                              Grid.Row="1"
                              Padding="0,0,0,0"
                              SelectionChanged="TypeComboBox_SelectionChanged">
                        <ComboBoxItem Content="NPC" />
                        <ComboBoxItem Content="Mortal" />
                        <ComboBoxItem Content="Ephemeral" />
                        <ComboBoxItem Content="Vampire" />
                        <ComboBoxItem Content="Mage" />
                        <ComboBoxItem Content="Werewolf" />
                    </ComboBox>
                    <mah:NumericUpDown x:Name="swAgeNumUpDown"
                                       FontSize="16"
                                       Grid.Column="1"
                                       Grid.Row="2"
                                       UpDownButtonsWidth="20"
                                       ParsingNumberStyle="Integer"
                                       Minimum="1"
                                       Maximum="202100"
                                       TrackMouseWheelWhenMouseOver="True"
                                       Style="{StaticResource MaterialDesignNumericUpDown}"
                                       RenderTransformOrigin="0.5,0.5"
                                       Padding="0,0,0,0"
                                       Foreground="White"
                                       TextAlignment="Left"
                                       ValueChanged="NumUpDown_ValueChanged" />
                    <ComboBox x:Name="swVirtueComboBox"
                              FontSize="16"
                              Grid.Column="1"
                              Grid.Row="3"
                              Padding="0,0,0,0"
                              IsEditable="True"
                              LostFocus="ComboBox_LostFocus">
                        <ComboBoxItem Content="Ambitious" />
                        <ComboBoxItem Content="Courageous" />
                        <ComboBoxItem Content="Generous" />
                        <ComboBoxItem Content="Honest" />
                        <ComboBoxItem Content="Hopeful" />
                        <ComboBoxItem Content="Humble" />
                        <ComboBoxItem Content="Just" />
                        <ComboBoxItem Content="Loving" />
                        <ComboBoxItem Content="Loyal" />
                        <ComboBoxItem Content="Patient" />
                        <ComboBoxItem Content="Peaceful" />
                        <ComboBoxItem Content="Righteous" />
                        <ComboBoxItem Content="Trustworthy" />
                    </ComboBox>
                    <ComboBox x:Name="swViceComboBox"
                              FontSize="16"
                              Grid.Column="1"
                              Grid.Row="4"
                              Padding="0,0,0,0"
                              IsEditable="True">
                        <ComboBoxItem Content="Ambitious" />
                        <ComboBoxItem Content="Arrogant" />
                        <ComboBoxItem Content="Corrupt" />
                        <ComboBoxItem Content="Cowardly" />
                        <ComboBoxItem Content="Cruel" />
                        <ComboBoxItem Content="Deceitful" />
                        <ComboBoxItem Content="Greedy" />
                        <ComboBoxItem Content="Hasty" />
                        <ComboBoxItem Content="Hateful" />
                        <ComboBoxItem Content="Pessimistic" />
                        <ComboBoxItem Content="Treacherous" />
                        <ComboBoxItem Content="Untrustworthy" />
                        <ComboBoxItem Content="Violent" />
                    </ComboBox>
                    <TextBox x:Name="swConceptTextBox"
                             FontSize="16"
                             Grid.Column="1"
                             Grid.Row="5"
                             Padding="0,0,0,0"
                             TextChanged="TextBox_TextChanged">
                    </TextBox>
                    <Label x:Name="swHealthLabel"
                           Grid.Column="0"
                           Grid.Row="6"
                           Grid.ColumnSpan="2"
                           Content="Health"
                           FontSize="20"
                           Padding="0,1,0,0"
                           FontFamily="Elephant"
                           HorizontalAlignment="Center" />
                    <md:RatingBar x:Name="swHealthDotsTop"
                                  Visibility="Hidden"
                                  Max="0"
                                  Grid.Row="7"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0"
                                  ValueChanged="Dots_ValueChanged">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxMarked"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <md:RatingBar x:Name="swHealthDotsBottom"
                                  Max="10"
                                  ValueChanged="Dots_ValueChanged"
                                  Grid.Row="7"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0"
                                  Foreground="#FF707070">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxBlank"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <Grid x:Name="swHealthStateGrid"
                          Grid.Column="0"
                          Grid.Row="8"
                          Grid.ColumnSpan="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="20" />
                        </Grid.ColumnDefinitions>
                        <Image x:Name="swHealthState1"
                               Grid.Column="1"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState2"
                               Grid.Column="2"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState3"
                               Grid.Column="3"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState4"
                               Grid.Column="4"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState5"
                               Grid.Column="5"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState6"
                               Grid.Column="6"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState7"
                               Grid.Column="7"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState8"
                               Grid.Column="8"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState9"
                               Grid.Column="9"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                        <Image x:Name="swHealthState10"
                               Grid.Column="10"
                               MouseLeftButtonDown="HealthState_LeftClick"
                               MouseRightButtonDown="HealthState_RightClick" />
                    </Grid>
                    <Label x:Name="swWillpowerLabel"
                           Grid.Column="0"
                           Grid.Row="9"
                           Grid.ColumnSpan="2"
                           Content="Willpower"
                           FontSize="20"
                           Padding="0,1,0,0"
                           FontFamily="Elephant"
                           HorizontalAlignment="Center" />
                    <md:RatingBar x:Name="swWillpowerDotsTop"
                                  Visibility="Hidden"
                                  Max="0"
                                  ValueChanged="Dots_ValueChanged"
                                  Grid.Row="10"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxMarked"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <md:RatingBar x:Name="swWillpowerDotsBottom"
                                  Foreground="#FF707070"
                                  Max="10"
                                  ValueChanged="Dots_ValueChanged"
                                  Grid.Row="10"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxBlank"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <Grid x:Name="swWillpowerValueGrid"
                          Grid.Column="0"
                          Grid.Row="11"
                          Grid.ColumnSpan="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="20" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="24" />
                            <ColumnDefinition Width="20" />
                        </Grid.ColumnDefinitions>
                        <CheckBox x:Name="swWillpowerCheck1"
                                  Grid.Column="1"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck2"
                                  Grid.Column="2"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck3"
                                  Grid.Column="3"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck4"
                                  Grid.Column="4"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck5"
                                  Grid.Column="5"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck6"
                                  Grid.Column="6"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck7"
                                  Grid.Column="7"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck8"
                                  Grid.Column="8"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck9"
                                  Grid.Column="9"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                        <CheckBox x:Name="swWillpowerCheck10"
                                  Grid.Column="10"
                                  md:CheckBoxAssist.CheckBoxSize="24"
                                  Checked="CheckBox_Checked" />
                    </Grid>
                    <Label x:Name="swIntegrityLabel"
                           Grid.Column="0"
                           Grid.Row="12"
                           Grid.ColumnSpan="2"
                           Content="Integrity"
                           FontSize="20"
                           Padding="0,1,0,0"
                           FontFamily="Elephant"
                           HorizontalAlignment="Center" />
                    <md:RatingBar x:Name="swIntegrityDotsTop"
                                  Visibility="Hidden"
                                  Max="0"
                                  ValueChanged="Dots_ValueChanged"
                                  Grid.Row="13"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxMarked"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <md:RatingBar x:Name="swIntegrityDotsBottom"
                                  Foreground="#FF707070"
                                  Max="10"
                                  ValueChanged="Dots_ValueChanged"
                                  Grid.Row="13"
                                  Grid.ColumnSpan="2"
                                  HorizontalAlignment="Left"
                                  Padding="20,0,0,0">
                        <md:RatingBar.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="Clear"
                                          Click="Clear_OnClick"
                                          CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Parent}" />
                            </ContextMenu>
                        </md:RatingBar.ContextMenu>
                        <md:RatingBar.ValueItemTemplate>
                            <DataTemplate DataType="system:Int32">
                                <Grid>
                                    <md:PackIcon Kind="RadioboxBlank"
                                                 Height="24"
                                                 Width="24" />
                                </Grid>
                            </DataTemplate>
                        </md:RatingBar.ValueItemTemplate>
                    </md:RatingBar>
                    <Label x:Name="swSizeLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Size:"
                           Grid.Column="0"
                           Grid.Row="14"
                           Padding="0,0,0,0" />
                    <Label x:Name="swSpeedLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Speed:"
                           Grid.Column="0"
                           Grid.Row="15"
                           Padding="0,0,0,0" />
                    <Label x:Name="swDefenseLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Defense:"
                           Grid.Column="0"
                           Grid.Row="16"
                           Padding="0,0,0,0" />
                    <Label x:Name="swArmorLabel"
                           FontSize="16"
                           VerticalAlignment="Bottom"
                           Foreground="White"
                           Content="Armor:"
                           Grid.Column="0"
                           Grid.Row="17"
